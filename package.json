{
  "name": "svgo-jsx-mcp",
  "version": "0.1.0",
  "private": true,
  "description": "SVG optimization with JSX-compatible camelCase conversion - Monorepo",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "dev:api": "pnpm --filter @svgo-jsx/api dev",
    "dev:web": "pnpm --filter @svgo-jsx/web dev",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r exec rm -rf dist node_modules"
  },
  "keywords": [
    "mcp",
    "svg",
    "svgo",
    "optimization",
    "jsx",
    "react"
  ],
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=20"
  }
}
