{
  "name": "@svgo-jsx/shared",
  "version": "0.1.0",
  "description": "Shared types and utilities for SVGO JSX",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./generators": {
      "types": "./dist/generators/index.d.ts",
      "import": "./dist/generators/index.js"
    },
    "./generators/types": {
      "types": "./dist/generators/types.d.ts",
      "import": "./dist/generators/types.js"
    },
    "./generators/react": {
      "types": "./dist/generators/react.d.ts",
      "import": "./dist/generators/react.js"
    },
    "./generators/vue": {
      "types": "./dist/generators/vue.d.ts",
      "import": "./dist/generators/vue.js"
    },
    "./generators/svelte": {
      "types": "./dist/generators/svelte.d.ts",
      "import": "./dist/generators/svelte.js"
    },
    "./generators/web-component": {
      "types": "./dist/generators/web-component.d.ts",
      "import": "./dist/generators/web-component.js"
    },
    "./validator": {
      "types": "./dist/validator.d.ts",
      "import": "./dist/validator.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "svgo": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "typescript": "^5.9.2"
  }
}
