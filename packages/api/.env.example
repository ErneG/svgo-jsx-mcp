# Database connection
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/svgo

# API Keys (comma-separated, for initial seeding)
API_KEYS=your-api-key-here

# Server port
PORT=3001

# CORS origins (comma-separated, or * for all)
CORS_ORIGINS=*

# Admin secret (shared with web app for admin API calls)
ADMIN_SECRET=your-admin-secret-here
